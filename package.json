{
	"name": "neotraverse",
	"version": "0.0.2",
	"description": "traverse and transform objects by visiting every node on a recursive walk",
	"main": "dist/legacy/legacy.cjs",
	"type": "module",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": {
				"production": "./dist/min/index.js",
				"development": "./src/index.js",
				"default": "./dist/index.js"
			},
			"default": "./dist/min/index.js"
		},
		"./legacy": {
			"node": "./dist/legacy/legacy.cjs",
			"types": "./dist/legacy/legacy.d.ts",
			"import": "./dist/legacy/legacy.js",
			"default": "./dist/legacy/legacy.js"
		},
		"./package.json": "./package.json"
	},
	"scripts": {
		"compile": "tsup",
		"test": "vitest run"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/puruvj/neotraverse.git"
	},
	"homepage": "https://github.com/puruvj/neotraverse",
	"keywords": [
		"traverse",
		"walk",
		"recursive",
		"map",
		"forEach",
		"deep",
		"clone"
	],
	"author": {
		"name": "Puru Vijay, James Halliday"
	},
	"license": "MIT",
	"engines": {
		"node": ">= 18"
	},
	"devDependencies": {
		"@types/node": "^20.14.10",
		"terser": "^5.31.2",
		"tsup": "^8.1.0",
		"typescript": "^5.5.3",
		"vitest": "^2.0.1"
	}
}
